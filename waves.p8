pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- pico wave
-- by jon ericson

--arena data
a = 63
background = 0 --black
boundary = 1 --dark_blue

--player data
x = 0 y = 0 r = 2
dx = 0 dy = 0
c = 12 --blue

--player bullet data
bullets = {}

--enemy data
enemies = {}
counter = 0

function _init()
  cls()
  --u60=_update60 _update60=nil function _update() u60() u60() end
  --camera(a, a)
end

function _update60()
  --counter-=1
  if (counter <= 0) then
     spawn()
     counter = 10+flr(rnd(30))
  end
  player_input()
end

function spawn()
  local enemy = {
    r = 1
  }
  repeat
    enemy.x = a-flr(rnd(a*2)) 
    enemy.y = a-flr(rnd(a*2))
  until (bounds(enemy.x,enemy.y,enemy.r))
  
  add(enemies, enemy)
end

function player_input()
  camera()
  
  dx = 0 dy = 0

  if (btn(0,1)) dx-=1
  if (btn(1,1)) dx+=1 
  if (btn(2,1)) dy-=1 
  if (btn(3,1)) dy+=1 

  x,y = move(x, dx, y, dy, r)

  --fire a bullet
 
  local bx = 0 by = 0

  if (btn(0)) bx-=2
  if (btn(1)) bx+=2
  if (btn(2)) by-=2
  if (btn(3)) by+=2
 
  if (bx ~= 0 or by ~= 0) then
    local bullet = {}
    bullet.dx = bx
    bullet.dy = by
    bullet.x,bullet.y 
     = move(x, bx, y, by, 0)
    bullet.c = 9
    --print(bullet.x, bullet,y)
    
    add(bullets, bullet)
  end
 
  --move the bullets
  for b in all(bullets) do
      if (not bounds(b.x+b.dx,b.y+b.dy, 0)) then
	       del(bullets, b)
      else
        b.x,b.y
         = move(b.x,b.dx,b.y,b.dy,0)
	     end
  end
  
  if (btnp(5)) camera(1,1)
  if (btnp(4)) spawn()
  --if (btnp(4)) c+=1
end

function move(x, dx, y, dy, r)
 if (bounds(x+dx, y+dy, r)) then
    x+=dx 
    y+=dy
 end
 return x,y
end

function _draw()
  --camera()
  -- arena
  circfill(a,a,a,background)
  circ(a,a,a,boundary)
  
  --camera(a,a)
  
  -- player
  circfill(x+a,y+a,r,c)
  --enemies
  --cls() print(#enemies)
  for e in all(enemies) do
      circ(e.x+a,e.y+a,e.r,8)
  end
  -- bullets
  --circ(x+a,y+a,r,boundary)
  --cls() print(#bullets)
  for b in all(bullets) do
      pset(b.x+a, b.y+a, b.c)
  end
end

function bounds(x, y, r)
  --print(sqrt(x*x+y*y)+r)
  if (sqrt(x*x+y*y)+r >= a) then
     return false
  else
     return true
  end
end
